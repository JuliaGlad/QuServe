        <item name="fontFamily">@font/regular</item>
        <item name="android:fontFamily">@font/bold_roboto</item>
    </style>
    
    <style name="TextAppearance.Material3.HeadlineMedium.QuServe" parent="TextAppearance.Material3.HeadlineMedium">
