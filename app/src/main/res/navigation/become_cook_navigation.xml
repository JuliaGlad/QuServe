    android:id="@+id/become_cook_navigation.xml"
    app:startDestination="@id/becomeCookFragment">

    <fragment
        android:id="@+id/becomeCookFragment"
        android:name="com.example.myapplication.presentation.employee.becomeCook.main.BecomeCookFragment"
        android:label="BecomeCookFragment" >
