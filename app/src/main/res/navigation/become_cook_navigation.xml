            android:id="@+id/action_becomeCookFragment_to_successfullyBecomeCookFragment"
            app:destination="@id/successfullyBecomeCookFragment"
            app:enterAnim="@anim/slide_in_fast_anim"
            app:exitAnim="@anim/slide_out_fast_anim"
