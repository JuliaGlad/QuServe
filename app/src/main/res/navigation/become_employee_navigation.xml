<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/become_employee_navigation"
    app:startDestination="@id/becomeEmployeeFragment">

    <fragment
        android:id="@+id/becomeEmployeeFragment"
        android:name="com.example.myapplication.presentation.employee.becomeEmployee.BecomeEmployeeFragment"
        android:label="BecomeEmployeeFragment" >
        <action
            android:id="@+id/action_becomeEmployeeFragment_to_successfullyBecomeEmployeeFragment"
            app:destination="@id/successfullyBecomeEmployeeFragment"
            app:enterAnim="@anim/slide_in_fast_anim"
            app:exitAnim="@anim/slide_out_fast_anim"
            app:popEnterAnim="@anim/slide_in_fast_anim"
            app:popExitAnim="@anim/slide_out_fast_anim"/>
    </fragment>

    <fragment
        android:id="@+id/successfullyBecomeEmployeeFragment"
        android:name="com.example.myapplication.presentation.employee.becomeEmployee.successfullyBecomeEmployee.SuccessfullyBecomeEmployeeFragment"
        tools:layout="@layout/fragment_successfuly_become_employee" />
</navigation>